<Window
    x:Class="StarBound_Legacy.Garage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:StarBound_Legacy"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="Garage"
    Width="1100"
    Height="619"
    ResizeMode="NoResize"
    WindowStartupLocation="CenterScreen"
    WindowState="Maximized"
    WindowStyle="None"
    mc:Ignorable="d">
    <Window.Background>
        <ImageBrush ImageSource="/Fond.png"/>
    </Window.Background>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="32*"/>
            <ColumnDefinition Width="94*"/>
            <ColumnDefinition Width="73*"/>
            <ColumnDefinition Width="29*"/>
            <ColumnDefinition Width="29*"/>
            <ColumnDefinition Width="29*"/>
            <ColumnDefinition Width="29*"/>
            <ColumnDefinition Width="29*"/>
            <ColumnDefinition Width="29*"/>
            <ColumnDefinition Width="29*"/>
            <ColumnDefinition Width="27*"/>
            <ColumnDefinition Width="29*"/>
            <ColumnDefinition Width="29*"/>
            <ColumnDefinition Width="204*"/>
            <ColumnDefinition Width="204*"/>
            <ColumnDefinition Width="204*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="41*"/>
            <RowDefinition Height="230*"/>
            <RowDefinition Height="87*"/>
            <RowDefinition Height="92*"/>
        </Grid.RowDefinitions>
        <Button Grid.Column="15" Content="Retour" Margin="30,30,30,30" Grid.Row="3" Click="Retour"/>
        <Button Grid.Column="1" Content="+" HorizontalAlignment="Right" Width="50" Height="50" VerticalAlignment="Center" Click="Button_Click" FontSize="24" FontWeight="Bold" Margin="35,0,0,0"/>
        <Button Grid.Column="2" Content="200 pts" HorizontalAlignment="Left" Width="100" VerticalAlignment="Center" Height="50" FontSize="24" FontWeight="Bold"/>
        <Rectangle Grid.Column="3" Stroke="Black" Fill="Red" Width="50" Height="50"/>
        <Rectangle Grid.Column="4" Stroke="Black" Fill="Red" Width="50" Height="50"/>
        <Rectangle Grid.Column="5" Stroke="Black" Fill="Red" Width="50" Height="50"/>
        <Rectangle Grid.Column="6" Stroke="Black" Fill="Red" Width="50" Height="50"/>
        <Rectangle Grid.Column="7" Stroke="Black" Fill="Red" Width="50" Height="50"/>
        <Rectangle Grid.Column="8" Stroke="Black" Fill="Red" Width="50" Height="50"/>
        <Rectangle Grid.Column="9" Stroke="Black" Fill="Red" Width="50" Height="50"/>
        <Rectangle Grid.Column="10" Stroke="Black" Fill="Red" Width="50" Height="50"/>
        <Rectangle Grid.Column="11" Stroke="Black" Fill="Red" Width="50" Height="50"/>
        <Rectangle Grid.Column="12" Stroke="Black" Fill="Red" Width="50" Height="50"/>

    </Grid>
</Window>
