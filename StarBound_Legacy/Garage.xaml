<Window
    x:Class="StarBound_Legacy.Garage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:StarBound_Legacy"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="Garage"
    Width="800"
    Height="450"
    ResizeMode="NoResize"
    WindowStartupLocation="CenterScreen"
    WindowState="Maximized"
    WindowStyle="None"
    mc:Ignorable="d">
    <Window.Background>
        <ImageBrush ImageSource="/Fond.png"/>
    </Window.Background>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="346*"/>
            <ColumnDefinition Width="140*"/>
            <ColumnDefinition Width="140*"/>
            <ColumnDefinition Width="140*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="41*"/>
            <RowDefinition Height="230*"/>
            <RowDefinition Height="87*"/>
            <RowDefinition Height="92*"/>
        </Grid.RowDefinitions>
        <Rectangle HorizontalAlignment="Left" Height="30" Margin="53,0,0,0" Stroke="Black" VerticalAlignment="Center" Width="30" Fill="Red"/>
        <Rectangle HorizontalAlignment="Left" Height="30" Margin="88,0,0,0" Stroke="Black" VerticalAlignment="Center" Width="30" Fill="Red"/>
        <Rectangle HorizontalAlignment="Left" Height="30" Margin="123,0,0,0" Stroke="Black" VerticalAlignment="Center" Width="30" Fill="Red"/>
        <Rectangle HorizontalAlignment="Left" Height="30" Margin="158,0,0,0" Stroke="Black" VerticalAlignment="Center" Width="30" Fill="Red"/>
        <Rectangle HorizontalAlignment="Left" Height="30" Margin="193,0,0,0" Stroke="Black" VerticalAlignment="Center" Width="30" Fill="Red"/>
        <Rectangle HorizontalAlignment="Left" Height="30" Margin="228,0,0,0" Stroke="Black" VerticalAlignment="Center" Width="30" Fill="Red"/>
        <Rectangle HorizontalAlignment="Left" Height="30" Margin="263,0,0,0" Stroke="Black" VerticalAlignment="Center" Width="30" Fill="Red"/>
        <Rectangle HorizontalAlignment="Left" Height="30" Margin="298,0,0,0" Stroke="Black" VerticalAlignment="Center" Width="30" Fill="Red"/>
        <Rectangle HorizontalAlignment="Left" Height="30" Margin="333,0,0,0" Stroke="Black" VerticalAlignment="Center" Width="30" Fill="Red" Grid.ColumnSpan="2"/>
        <Rectangle HorizontalAlignment="Left" Height="30" Stroke="Black" VerticalAlignment="Center" Width="48" Fill="#FF888888"/>
        <Button Grid.Column="3" Content="Retour" Margin="30,30,30,30" Grid.Row="3" Click="Retour"/>

    </Grid>
</Window>
